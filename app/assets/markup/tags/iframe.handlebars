<iframe
    {{#if this.type}}
    type="{{this.type}}"
    {{/if}}
    {{#if this.class}}
    class="{{this.class}}"
    {{/if}}
    {{#if this.id}}
    id="{{this.id}}"
    {{/if}}
    {{#if this.title}}
    title="{{this.title}}"
    {{/if}}
    {{#if this.url}}
    src="{{this.url}}"
    {{/if}}
></iframe>
